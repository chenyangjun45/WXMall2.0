/* 券列表 */
.integralStoreList{
  text-align:left;
  border-bottom: 1px solid #eee;
}
.list_des{
  height: 140rpx;
  line-height: 140rpx;
  padding-left: 48rpx;
  font-size: 60rpx;
}
.list_item{
  padding-top: 40rpx;
  height: 320rpx;
  display:flex;
}
.item_left{
  background-color: #F0616E;
  display:flex;
  height: 320rpx;
  width:330rpx;
}
.first{
  /* position: absolute;  */
  width: 230rpx;
  height: 320rpx;
  line-height: 320rpx;
  font-size: 180rpx;
  font-weight: 900;
  text-align: center;
  color: #fff;
  /* top: 20px;
  left: 0; */
  z-index: 1;
}
.second{
  /* position: relative; */
  height:240rpx;
  /* 注意指定了line-height以后文字就在最中间 */
  line-height: 240rpx; 
   width:100rpx; 
  text-align: left;
  font-size: 30px;
  color: #fff;
  padding-top:80rpx;
  /* padding-left:10rpx; */
}
.item_right{
  height: 320rpx;
  background-color: #eeeeee;
  width:420rpx; 
  display:flex;
}
.bg{
  position:relative;
  left:-20rpx;
  width: 24rpx;
  height: 320rpx;
  /* 不要用background,这个只能用网络资源，在手机上显示不出来 */
  /* background: transparent url(../resources/images/sawtooth.png) repeat-y left; */
}
.ticket_info{
  /* padding-left: 20rpx;  */
  width:420rpx;
}
.ticket_info1{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 40rpx;
  font-weight: bold;
   margin-top: 10px; 
}
.ticket_info2{
  /* 取消height，解决文字重叠问题 */
  /* height: 40rpx; */
  line-height: 40rpx;
  font-size: 30rpx;
}
.movie_pic{
  height:320rpx;
  width:330rpx;
}
.item_left_green{
  background-color: green;
  display:flex;
  width:330rpx;
}