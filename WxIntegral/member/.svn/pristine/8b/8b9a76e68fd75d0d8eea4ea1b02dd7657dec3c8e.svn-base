/* pages/user/setCardNo/setCardNo.wxss */

.inteval {
   width: 100%;
   height: 20rpx;
   background-color: #eee;
}

.inputModule {
   width: 100%;
   height: 90rpx;
   line-height: 80rpx;
   display: flex;
   padding: 0 24px;
}

.cardtext {
   color: #626262;
   height: 90rpx;
   line-height: 90rpx;
   font-size: 38rpx;
   padding-right: 10rpx;
}

.region {
   background-color: #939393;
   color: #fff;
   height: 50rpx;
   line-height: 50rpx;
   font-size: 40rpx;
   margin: 20rpx 10rpx;
}

.cardinput {
   height: 50rpx;
   margin-top: 20rpx;
   margin-left: 20rpx;
}

.tips {
   font-size: 35rpx;
   padding: 0 24px;
   margin: 20px 0;
}

.button {
   color: #fff;
   font-size: 50rpx;
   height: 100rpx;
   line-height: 100rpx;
   background-color: #50afff;
   margin: 100rpx 30rpx 40rpx 30rpx;
}

.city-choose {
   margin: 0 20rpx;
   align-items: center;
}

.city-grid {
   background-color: #dedede;
   float: left;
   width: 99rpx;
   height: 160rpx;
   text-align: center;
   color: #626262;
   margin: 10rpx;
}

.ischecked {
   background-color: #939393;
   color: #fff;
}

.lable {
   font-size: 32rpx;
}

.abbrname-lable {
   margin: 30rpx 0 0 0;
   font-size: 40rpx;
}

.cityname-lable {
   margin: 20rpx 0 30rpx 0;
}
