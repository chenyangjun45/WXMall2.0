/* pages/store/myCoupon/myCoupon.wxss */
@import "../../templates/smallLoaderTemplate.wxss"; 
.inteval{
 width:100%;
 height:20rpx;
 background-color: #EEEEEE;

}
/*
导航栏样式
*/
.swiper-tab{
  display: flex;
  flex-direction: row;
  width:100%;
  height: 80rpx;
  border-bottom: 5rpx solid #eee;
}
.tab-item {
  margin:5rpx 15rpx 0 5rpx;
  font-size: 38rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.on {  
  color: #50AFFF;  
  border-bottom: 3rpx solid #50AFFF;  
}  
/**
优惠券列表样式
*/
.view1{
 width:100%;
 height:1000rpx;
 overflow: auto;
}

.drow_down{
  text-align: center;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  color: #888;
  font-family: Arial, Microsoft YaHei;
}
.drow_up{
  text-align: center;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  color: #888;
  font-family: Arial, Microsoft YaHei;
}
::-webkit-scrollbar{
  height:0;
  width:0;
  color:transparent;
}
.coupon-list{
  display: flex;
  flex-direction: row;
  height:200rpx;
  width:100%;
  background: #F3F3F3;
  margin-top:20rpx;
 


}
.item_left{
 
  height:200rpx;
  width:250rpx;

}
.dis_item_left{
 background-color: #F0616E;
}
.integralStoreimg{
 width:250rpx;
 height:200rpx;
}
.sale_item_left{
  background-color: green;
}
.time_item_left{
    background-color: green;
}
.price{
  line-height: 200rpx;
  font-size: 120rpx;
  font-weight: 900;
  margin-left: 30rpx;
  color: #fff;
}
.sawtoothimg{
  width:10rpx;
  height:200rpx;
  z-index:20;
  position: relative;
  left:-5rpx;
}
.mem{
 width:440rpx;
}
.title{
  font-size:30rpx;
  margin:20rpx 0 20rpx 20rpx;
}
.detail{
   font-size: 25rpx;
   margin-left:20rpx;
   color: #626262;
}


.end_tips{
  font-size: 25rpx;
  
  text-align: center;
  margin :20rpx 0 0 300rpx;

}
/*
底部样式
*/
.foot{
  width: 100%;
  height: 80rpx;
  position: fixed;
  bottom: 10rpx;
  
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
}
.footext{
    height: 30rpx;
    line-height: 30rpx;
    margin: 25rpx 0;
    width:50%;
}
.footext1{
    text-align: right;
    padding-right: 20rpx;
    border-right: 1rpx solid #626262;
    color: #c3c3c3;
}
.footext2{
    padding-left: 20rpx;
}